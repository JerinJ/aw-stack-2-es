{
  "name": "aw-stack-2-es",
  "version": "0.0.1",
  "repository": "https://github.com/aw-stacks/aw-stack-2-es.git",
  "author": "hegdeashwin <ashwin.hegde3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "webpack-dev-server --content-base ./dist --config \"./configs/build/dev.webpack.config.js\" --progress",
    "start:hot": "yarn run start:dev -- --inline --hot",
    "start:prod": "webpack-dev-server --content-base ./dist --config \"./configs/build/prod.webpack.config.js\" --progress",
    "bundle:dev": "yarn run clean:dist && webpack --config \"./configs/build/dev.webpack.config.js\" --progress -p -d",
    "bundle:watch": "yarn run clean:dist && webpack --config \"./configs/build/dev.webpack.config.js\" --progress -d --watch",
    "build": "yarn run lint && yarn run test && yarn run clean:dist && webpack --config \"./configs/build/prod.webpack.config.js\" --progress -p -d",
    "lint": "eslint public configs routes",
    "lint:fix": "eslint --fix public configs routes",
    "test": "",
    "clean:all": "yarn run clean:dist && yarn run clean:test && yarn run clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf test",
    "clean:coverage": "rimraf coverage",
    "check:package": "yarn outdated",
    "docs:code": "jsdoc -d docs configs src && yarn run docs:code:start",
    "docs:code:start": "http-server ./docs"
  },
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.9",
    "rxjs": "5.0.3",
    "zone.js": "0.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "jasmine-core": "2.5.2",
    "jsdoc": "^3.4.3",
    "karma": "1.4.0",
    "karma-coverage": "1.1.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "2.0.1",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "2.4.0"
  }
}
